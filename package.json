	"scripts": {
		"build": "wp-scripts build --mode production",
		"format": "wp-scripts format",
		"lint:js": "wp-scripts lint-js",
		"lint:css": "wp-scripts lint-style",
		"lint:php": "composer run lint",
		"plugin-zip": "wp-scripts plugin-zip",
		"start": "wp-scripts start --progress --mode development",
		"start:debug": "DEBUG_MODE=on wp-scripts start --progress",
		"packages-update": "wp-scripts packages-update",
		"clean": "rm -rf build dist/* node_modules vendor wordpress",
		"prepare": "husky install",
		"husky:init": "npx husky-init && chmod ug+x .husky/*",
		"flow": "flow ./src",
		"setup": "yarn install && yarn run build",
		"test:e2e": "cypress run --config video=false --browser chrome",
		"test:e2e:debug": "npx cypress open",
		"test:e2e:headed": "yarn test:e2e --headed",
		"test:e2e:record": "cypress run --browser chrome",
		"test:js": "wp-scripts test-unit-js --config .dev/tests/jest/jest.config.js",
		"test:js:coverage": "yarn test:js --coverage",
		"test:js:update": "yarn test:js -u",
		"test:generate-output": "jest --json --outputFile=.jest-test-results.json",
		"test:generate-output:dev": "jest --json --outputFile=.jest-test-results.json -- --watch",
		"version": "grunt version && grunt update-pot",
		"storybook:init": "npx storybook@latest init --type react",
		"storybook:start": "storybook dev -p 6008",
		"storybook:build": "storybook build",
		"storybook:test": "test-storybook",
		"storybook:test:start": "test-storybook --watch",
		"icons:indexing": "node packages/components/bin/icons/index.js search.json"
	},
	"lint-staged": {
		"*.{js,json,ts,tsx,yml,yaml}": [
			"wp-scripts format"
		],
		"*.{js,ts,tsx}": [
			"wp-scripts lint-js"
		],
		"*.scss": [
			"wp-scripts lint-style"
		]
	}
}
